<?xml version="1.0" encoding="UTF-8"?>
<!-- 絶対パスの方が望ましい. -->
<config>
	<!-- credentials.json -->
	<credentials>$GD/res/credentials.json</credentials>
	<!-- token.json (フォルダ) -->
	<token>$GD/res/token.json/</token>
	<!-- 一時保存 -->
	<temp>$GD/temp</temp>
	<!-- Unity Client Folder -->
	<client>$CL</client>
	<!-- Unity Client Assets Folder -->
	<assets>$CL/Assets</assets>
	<!-- PageSize Drive上にファイルが存在するがDL/UP出来ない場合に増やす -->
	<page>300</page>

	<!-- memo.増やすときは'data'タグ配列を増やす. -->
	<!-- アップロード関連 -->
	<upload>
		<data>
			<source></source>
			<destination></destination>
		</data>
	</upload>
	<!-- ダウンロード関連 -->
	<download>
		<data>
			<!--ドライブ上のダウンロード対象ファイル(.zip)-->
			<source>Template-OnlineProject/ProjectSetting/GRPC.zip</source>
			<!--Rootディレクトリ読み込まなかったのでカットver.-->

			<!--ローカル上の配置先-->
			<destination>$CL/Assets/Plugins/</destination>
		</data>
	</download>
</config>